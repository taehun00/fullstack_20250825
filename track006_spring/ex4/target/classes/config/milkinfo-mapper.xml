<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.thejoa703.dao.MilkInfoDao">
  <insert	id="insert"	parameterType="com.thejoa703.dto.MilkInfoDto">
  	insert into milk (mno, mname, mnum, mtotal) values (milk_seq.nextval, #{mname}, #{mnum}, #{mtotal} )
  </insert>
  <select resultType="MilkInfoDto" id="selectAll">
  	select * from milk
  </select>
  <select resultType="MilkInfoDto" id="select" parameterType="int">
  	select * from milk where mno=#{mno}
  </select>
  <update id="update" parameterType="com.thejoa703.dto.MilkInfoDto">
  	update milk set mname=#{mname}, mnum=#{mnum}, mtotal=#{mtotal} where mno=#{mno}
  </update>
  <delete id="delete" parameterType="int">
  	delete from milk where mno=#{mno}
  </delete>
</mapper>